<?xml version="1.0" encoding="utf-8"?>

<tag_based_concept xmlns="http://knx.org">

	<vocabulary>
	
	<marker name="unit" />
	<marker name="value" />
	<marker name="valueReal" />
	<marker name="datapoint" />
	<marker name="property" />
	<marker name="device" />

	<basic name="id" type="string"/>
	<basic name="name" type="string"/>
	<basic name="kelvin" type="int"/>
	<basic name="Symbol" type="string"/>
	<basic name="offset" type="string"/>
	<basic name="min" type="int"/>
	<basic name="max" type="int"/>
	<basic name="serialnumber" type="string"/>

	<reference name="unitRef" usedwith="celsius"/>
	<reference name="valueRef" usedwith="value"/>
	<reference name="typeRef" usedwith="type"/>
	<reference name="propertyRef" usedwith="property"/>
	<reference name="datapointRef" usedwith="datapoint"/>

	<composition tag="celsius" occurrence="many" usedwith="unit"/>


	</vocabulary>
	
	<informationmodel>
	
	<entity>
	<feature tag="id" value="celsius"/>
	<feature tag="unit"/>
	<feature tag="name" value="Celsius"/>
	<feature tag="symbol" value="C"/>
	<feature tag="offset" value="-273.15"/>
	<feature tag="kelvin" value="1"/>
    </entity>
	
	
	
	<entity>
	<feature tag="id" value="temp_val"/>
	<feature tag="value"/>
	<feature tag="valueReal"/>
	<feature tag="name" value="Temperature"/>
	<feature tag="min" value="-273"/>
	<feature tag="max" value="670760"/>
	<feature tag="unitRef" value="celsius"/>
    </entity>
	
	<entity>
	<feature tag="id" value="temp_type"/>
	<feature tag="type"/>
	<feature tag="name" value="Temperature type"/>
	<feature tag="valueRef" value="temp_val"/>
    </entity>
	
	<entity>
	<feature tag="id" value="temperature_dp"/>
	<feature tag="datapoint"/>
	<feature tag="name" value="Temperature temperature"/>
	<feature tag="typeRef" value="temp_type"/>
    </entity>
	
	
	<entity>
	<feature tag="id" value="serial"/>
	<feature tag="property"/>
	<feature tag="serialnumber" value="AB13"/>
    </entity>
	
	<entity>
	<feature tag="id" value="temperature_controller"/>
	<feature tag="device"/>
	<feature tag="name" value="Temperature controller"/>
	<feature tag="propertyRef" value="serial"/>
	<feature tag="datapointRef" value="temperature_dp"/>
	</entity>
	

	
	</informationmodel>
	
	
</tag_based_concept>